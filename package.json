{
	"name": "bun-ts-project-template",
	"module": "src/index.ts",
	"type": "module",
	"devDependencies": {
		"@biomejs/biome": "^2.3.14",
		"@types/bun": "^1.3.8",
		"@typescript/native-preview": "^7.0.0-dev.20260204.1",
		"typescript": "^5.9.3"
	},
	"bin":{
		"myapptest": "./bin/main.ts"
		
	},
	"scripts": {
		"dev": "bun --watch run.ts",
		"test": "bun test",
		"check": "biome check --write src/ && bun run typecheck",
		"format": "biome format --write src/",
		"typecheck": "tsgo --noEmit",
		"build": "bun build run.ts --outdir ./dist"
	}
}
